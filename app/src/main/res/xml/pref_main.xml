<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <Preference
        android:key="@string/pref_key_circleimg"
        android:title="圆角ImageView">
        <intent
            android:targetClass="com.duzhou.zlibray.circleImage.CircleImageActivity"
            android:targetPackage="com.duzhou.zlibray"></intent>
    </Preference>

    <Preference
        android:key="@string/pref_key_wifi"
        android:title="WIFI manager">
        <intent
            android:targetClass="com.duzhou.zlibray.wifi.WifiActivity"
            android:targetPackage="com.duzhou.zlibray"></intent>
    </Preference>


    <Preference
        android:key="@string/pref_key_security"
        android:title="加密">
        <intent
            android:targetClass="com.duzhou.zlibray.security.SecurityActivity"
            android:targetPackage="com.duzhou.zlibray"></intent>
    </Preference>

    <Preference
        android:key="@string/pref_key_spring"
        android:title="Springing view">
        <intent
            android:targetClass="com.duzhou.zlibray.springingview.SpringingActivity"
            android:targetPackage="com.duzhou.zlibray"></intent>
    </Preference>




</PreferenceScreen>