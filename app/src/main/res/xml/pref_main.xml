<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <Preference
        android:key="@string/pref_key_circleimg"
        android:title="圆角ImageView">
        <intent
            android:targetClass="com.duzhou.zlibray.circleImage.CircleImageActivity"
            android:targetPackage="com.duzhou.zlibray"></intent>
    </Preference>

    <Preference
        android:key="@string/pref_key_wifi"
        android:title="WIFI manager">
        <intent
            android:targetClass="com.duzhou.zlibray.wifi.WifiActivity"
            android:targetPackage="com.duzhou.zlibray"></intent>
    </Preference>


    <Preference
        android:key="@string/pref_key_security"
        android:title="加密">
        <intent
            android:targetClass="com.duzhou.zlibray.security.SecurityActivity"
            android:targetPackage="com.duzhou.zlibray"></intent>
    </Preference>

    <Preference
        android:key="@string/pref_key_clockview"
        android:title="自定义view-钟表">
        <intent
            android:targetClass="com.duzhou.zlibray.clockview.ClockActivity"
            android:targetPackage="com.duzhou.zlibray"></intent>
    </Preference>

    <Preference
        android:key="@string/pref_key_zbar"
        android:title="二维码条形码扫描">
        <intent
            android:targetClass="com.duzhou.zlibray.scan.ScanActivity"
            android:targetPackage="com.duzhou.zlibray"></intent>
    </Preference>


    <Preference
        android:key="@string/pref_key_spring"
        android:title="阻尼效果的动画">
        <intent
            android:targetClass="com.duzhou.zlibray.springingview.SpringingActivity"
            android:targetPackage="com.duzhou.zlibray"></intent>
    </Preference>


    <Preference
        android:key="@string/pref_key_bluetooth"
        android:title="Bluetooth">
        <intent
            android:targetClass="com.duzhou.zlibray.bluetooth.BlueToothActivity"
            android:targetPackage="com.duzhou.zlibray"></intent>
    </Preference>



</PreferenceScreen>