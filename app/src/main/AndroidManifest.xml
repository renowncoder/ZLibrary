<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.duzhou.zlibray">

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_MULTICAST_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <uses-permission android:name="android.permission.CAMERA" />

    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN"/>
    <uses-permission android:name="android.permission.BLUETOOTH"/>

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme1">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".circleImage.CircleImageActivity"></activity>
        <activity android:name=".wifi.WifiActivity"></activity>
        <activity android:name=".security.SecurityActivity" />
        <activity android:name=".springingview.SpringingActivity" />
        <activity android:name=".clockview.ClockActivity" />
        <activity android:name=".scan.ScanActivity" />
        <activity android:name=".scan.ZBarActivity" />
        <activity android:name=".scan.ZXingActivity" />
        <activity android:name=".bluetooth.BlueToothActivity" />
        <activity android:name=".bluetooth.BlueToothSocketActivity" />
    </application>


</manifest>